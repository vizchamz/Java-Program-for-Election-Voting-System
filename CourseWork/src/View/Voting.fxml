<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="624.0" prefWidth="1008.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.VotingController">
   <top>
      <AnchorPane fx:id="adminOptions" minHeight="0.0" minWidth="0.0" prefHeight="55.0" prefWidth="1008.0" stylesheets="@../img/style.css" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="633.0" layoutY="6.0" text="Presidential Election - 2020" textFill="WHITE">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <ImageView fitHeight="41.0" fitWidth="42.0" layoutX="8.0" layoutY="8.0" pickOnBounds="true">
               <image>
                  <Image url="@../img/292fd6a99e.png" />
               </image>
            </ImageView>
            <Label fx:id="lbl" layoutX="50.0" layoutY="18.0" text="voter" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </top>
   <center>
      <AnchorPane fx:id="adminOptionsBackground" minHeight="0.0" minWidth="0.0" prefHeight="558.0" prefWidth="1008.0" stylesheets="@../img/style.css" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="7.0" layoutY="548.0" text="US Presidential Election - 2020" textFill="WHITE" />
            <Button fx:id="submitVoterID" contentDisplay="CENTER" layoutX="101.0" layoutY="262.0" mnemonicParsing="false" onAction="#submitVoterID" prefHeight="45.0" prefWidth="137.0" style="-fx-background-color: none; -fx-border-color: white;" text="Vote" textFill="WHITE">
               <font>
                  <Font name="Arial" size="24.0" />
               </font>
            </Button>
            <TableView fx:id="tableView1" editable="true" layoutX="362.0" layoutY="85.0" prefHeight="399.0" prefWidth="616.0">
              <columns>
                <TableColumn fx:id="col_id" prefWidth="124.0" text="ID" />
                <TableColumn fx:id="col_name" prefWidth="342.0" text="Name" />
                  <TableColumn fx:id="col_party" prefWidth="149.0" text="Party" />
              </columns>
            </TableView>
            <TextField fx:id="voterTextField" layoutX="34.0" layoutY="196.0" prefHeight="32.0" prefWidth="271.0" promptText="Enter Candidate ID to Vote" style="-fx-background-color: none; -fx-border-color: white;">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <Button contentDisplay="CENTER" layoutX="101.0" layoutY="327.0" mnemonicParsing="false" onAction="#resetVoterID" prefHeight="45.0" prefWidth="137.0" style="-fx-background-color: none; -fx-border-color: white;" text="Reset" textFill="WHITE">
               <font>
                  <Font name="Arial" size="24.0" />
               </font>
            </Button>
            <TextField fx:id="voterTextField2" layoutX="34.0" layoutY="145.0" prefHeight="32.0" prefWidth="271.0" promptText="Enter Voter ID" style="-fx-background-color: none; -fx-border-color: white;">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
